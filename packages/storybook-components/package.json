  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@storybook/csf": "^0.1.1",
    "@storybook/theming": "^7.5.3",
    "global": "^4.4.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.pickby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "markdown-to-jsx": "^7.3.2",
    "polished": "^4.2.2",
    "react-textarea-autosize": "^8.5.3"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.pickby": "^4.6.9",
    "@types/lodash.uniq": "^4.5.9",
    "@types/node": "^18.11.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
