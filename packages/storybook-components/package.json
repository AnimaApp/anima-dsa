{
  "name": "@animaapp/storybook-components",
  "version": "1.0.0",
  "description": "Collection of Storybook components for Anima",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
