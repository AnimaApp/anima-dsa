import { type DesignTokenMap } from './types';
import { type DesignTokenValue } from './types/value';
interface RemovalDiff {
    type: 'REMOVAL';
    name: string;
    value: DesignTokenValue;
}
interface AdditionDiff {
    type: 'ADDITION';
    name: string;
    value: DesignTokenValue;
}
interface ModificationDiff {
    type: 'MODIFICATION';
    name: string;
    value: {
        before: DesignTokenValue;
        after: DesignTokenValue;
    };
}
export type TokenDiff = RemovalDiff | AdditionDiff | ModificationDiff;
interface Options {
    includeRemovals?: boolean;
    includeAdditions?: boolean;
    includeModifications?: boolean;
}
export declare const getDiffForTokens: (dtBefore: DesignTokenMap, dtAfter: DesignTokenMap, options?: Options) => TokenDiff[];
export {};
//# sourceMappingURL=getDiffForTokens.d.ts.map